<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>name</key>
    <string>libssh2-ios</string>
    <key>url</key>
    <string>https://github.com/IMcD23/libssh2-ios.git</string>
    <key>library</key>
    <dict>
        <key>url</key>
        <string>https://github.com/libssh2/libssh2.git</string>
        <key>branch</key>
        <string>master</string>
    </dict>
    <key>dependencies</key>
    <array>
        <dict>
            <key>url</key>
            <string>https://github.com/IMcD23/openssl-ios.git</string>
            <key>branch</key>
            <string>master</string>
        </dict>
    </array>
    <key>libraryOutputs</key>
    <array>
        <string>libssh2</string>
    </array>
    <key>buildSystem</key>
    <string>cmake</string>
    <key>buildArgs</key>
    <array>
        <string>-DBUILD_SHARED_LIBS=OFF</string>
        <string>-DBUILD_EXAMPLES=OFF</string>
        <string>-DBUILD_TESTING=OFF</string>
        <string>-DCRYPTO_BACKEND=OpenSSL</string>
        <string>-DOPENSSL_ROOT_DIR=$#BUILDROOT#</string>
    </array>
</dict>
</plist>